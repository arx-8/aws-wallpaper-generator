{
  "name": "re-ducks-example",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@emotion/core": "10.0.15",
    "emotion-normalize": "10.1.0",
    "immer": "3.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "ulid": "2.3.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/prettier": "1.18.1",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "4.3.4",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-jest": "22.14.1",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "source-map-explorer": "2.0.1",
    "typescript": "3.5.3",
    "typesync": "0.5.2"
  },
  "scripts": {
    "analyze:run": "source-map-explorer 'build/static/js/*.js' --html misc/source-map-explorer.html",
    "analyze": "npm-run-all --sequential build analyze:run",
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src > misc/eslint-print-config.json",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx src",
    "lint:tsc": "tsc --noEmit",
    "lint-dry": "npm-run-all --parallel lint:eslint-dry lint:tsc",
    "lint": "npm-run-all --parallel lint:eslint lint:tsc",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "typesync": "typesync"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
